{
  "name": "cezerin",
  "version": "0.33.0",
  "description": "Cezerin is React and Node.js based eCommerce platform. https://cezerin.com",
  "keywords": [
    "cezerin",
    "ecommerce",
    "shopping cart",
    "shop",
    "cms"
  ],
  "homepage": "https://cezerin.com",
  "bugs": "https://github.com/cezerin/cezerin/issues",
  "license": "MIT",
  "author": {
    "name": "Restmount <jsonmount@gmail.com>",
    "url": "https://github.com/restmount"
  },
  "contributors": [
    {
      "name": "Restmount <jsonmount@gmail.com>",
      "url": "https://github.com/restmount"
    },
    {
      "name": "Sergey Onufrienko <sergey@onufrienko.com>",
      "url": "https://github.com/sonufrienko"
    }
  ],
  "main": "src/api/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cezerin/cezerin"
  },
  "scripts": {
    "setup": "node -r esm src/api/server/setup.js",
    "compile": "babel src/store -d dist/store",
    "compile:watch": "concurrently -n storeWatcher,themeWatcher \"babel src/store -d dist/store --watch\" \"babel theme/src -d theme/dist --watch\"",
    "webpack:admin": "webpack --config webpack.config.admin.js --mode production",
    "webpack:store": "webpack --config webpack.config.store.js --mode production",
    "webpack:admin:watch": "webpack --config webpack.config.admin.js --mode development --watch",
    "webpack:store:watch": "webpack --config webpack.config.store.js --mode development --watch",
    "theme:install": "./scripts/theme-install.sh",
    "theme:export": "./scripts/theme-export.sh",
    "theme:build": "babel theme/src -d theme/dist && npm run webpack:store",
    "build": "npm run compile && npm run theme:build && npm run webpack:admin",
    "build:watch": "concurrently -n compile,webpackStore,webpackAdmin \"npm run compile:watch\" \"npm run webpack:store:watch\" \"npm run webpack:admin:watch\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"src/**/*.js\" \"theme/**/*.js\"",
    "precommit": "lint-staged",
    "start-api": "node -r esm src/api/server/index.js",
    "start-store": "node -r esm dist/store/server/index.js",
    "start": "concurrently npm:start-*"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.2.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^7",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "cezerin-client": "^0.34.0",
    "chart.js": "^2.7.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.1",
    "esm": "^3.0.80",
    "express": "^4.16.3",
    "express-jwt": "^5.3.0",
    "formidable": "^1.2.1",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.0.11",
    "helmet": "^3.13.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "jsonwebtoken": "^8.3.0",
    "lint-staged": "^9.2.0",
    "lodash": "^4.17.10",
    "lru-cache": "^5.1.1",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.22.2",
    "mongodb": "^3.3.0-beta2",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.9.3",
    "nodemailer": "^6.3.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.2",
    "react-dropzone": "^10.1.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scroll": "^1.7.10",
    "react-sortable-hoc": "^1.9.1",
    "react-stripe-elements": "^4.0.0",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.0",
    "redux-form": "^8.2.4",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.3.0",
    "response-time": "^2.3.2",
    "sass-loader": "^7.1.0",
    "sitemap": "^3.2.2",
    "slug": "^1.1.0",
    "stripe": "^7.4.0",
    "theme": "file:theme",
    "ua-parser-js": "^0.7.18",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "winston": "^3.0.0",
    "workbox-webpack-plugin": "^4.3.1",
    "ws": "^6.0.0"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": "^6.0.0"
  }
}
